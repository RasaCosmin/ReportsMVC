@model MVCReports.Models.AccuracyViewModel

<body>
        @using (Html.BeginForm("GenerateReport", "Home"))
        {
            <input type="text" id="datepicker" />
         <ul class="list-unstyled">
                @for (int i = 0; i < Model.Customers.Count; i++)
                {
                    @*<li><a href="#" id="a-client-list-item">@name</a></li>*@
                    <li>
                        @Html.CheckBoxFor(m => m.Customers[i].Checked, new { data_check = Model.Customers[i].Name})
                        @Html.HiddenFor(x => x.Customers[i].Name)
                        @Model.Customers[i].Name
                    </li>
                }
            </ul>
            <input  value="Generate Report" type="submit"/>
    }
</body>
<script>
  $(function() {
    $( "#datepicker" ).datepicker();
  });
</script>